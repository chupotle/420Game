<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.threebetasonematt.a420game">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".InitialActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartGameActivity" />
        <activity android:name=".HostLobbyActivity" />
        <activity android:name=".JoinLobbyActivity" />
        <activity android:name=".EnterAddressActivity" />
        <activity android:name=".GameActivity"></activity>
    </application>

</manifest>